{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    },
    {
      "src": "api/blog-posts.ts",
      "use": "@vercel/node"
    },
    {
      "src": "api/admin/blog-posts.ts",
      "use": "@vercel/node"
    },
    {
      "src": "api/blog-posts/[id].ts",
      "use": "@vercel/node"
    },
    {
      "src": "api/admin/blog-posts/[id].ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/googlec3cfbe8ec5429358.html",
      "dest": "/googlec3cfbe8ec5429358.html"
    },
    {
      "src": "/api/blog-posts",
      "dest": "/api/blog-posts.ts"
    },
    {
      "src": "/api/blog-posts/([^/]+)",
      "dest": "/api/blog-posts/[id].ts?id=$1"
    },
    {
      "src": "/api/admin/blog-posts/([^/]+)",
      "dest": "/api/admin/blog-posts/[id].ts?id=$1"
    },
    {
      "src": "/api/admin/blog-posts",
      "dest": "/api/admin/blog-posts.ts"
    },
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}